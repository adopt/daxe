      <PARAMETER name="font" value="Monospaced"/>
    </ELEMENT_DISPLAY>
    <ELEMENT_DISPLAY element="style" type="area">
      <PARAMETER name="titleAtt" value="type"/>
    </ELEMENT_DISPLAY>
    <ELEMENT_DISPLAY element="ul" type="wlist"/>
    <ELEMENT_DISPLAY element="li" type="witem"/>
    <ELEMENT_DISPLAY element="ol" type="wlist">
      <PARAMETER name="type" value="ol"/>
    </ELEMENT_DISPLAY>
    <ELEMENT_DISPLAY element="dl" type="area"/>
    <ELEMENT_DISPLAY element="dt" type="string"/>
    <ELEMENT_DISPLAY element="dd" type="string"/>
    <ELEMENT_DISPLAY element="table" type="table">
      <PARAMETER name="colspanAttr" value="colspan"/>
      <PARAMETER name="rowspanAttr" value="rowspan"/>
      <PARAMETER name="alignAttr" value="align"/>
    </ELEMENT_DISPLAY>
    <ELEMENT_DISPLAY element="tr" type="tr"/>
    <ELEMENT_DISPLAY element="td" type="td"/>
    <ELEMENT_DISPLAY element="th" type="th">
      <PARAMETER name="style" value="BOLD"/>
    </ELEMENT_DISPLAY>
    <ELEMENT_DISPLAY element="img" type="file">
      <PARAMETER name="srcAtt" value="src"/>
    </ELEMENT_DISPLAY>
    <ELEMENT_DISPLAY element="object" type="area"/>
    <ELEMENT_DISPLAY element="param" type="empty">
      <PARAMETER name="titleAtt" value="name"/>
    </ELEMENT_DISPLAY>
    <ELEMENT_DISPLAY element="form" type="area"/>
    <ELEMENT_DISPLAY element="input" type="empty"/>
    <ELEMENT_DISPLAY element="label" type="string"/>
    <ELEMENT_DISPLAY element="select" type="area"/>
    <ELEMENT_DISPLAY element="option" type="string"/>
    <ELEMENT_DISPLAY element="textarea" type="string"/>
    <ELEMENT_DISPLAY element="sup" type="style">
      <PARAMETER name="style" value="SUPERSCRIPT"/>
    </ELEMENT_DISPLAY>
    <ELEMENT_DISPLAY element="sub" type="style">
      <PARAMETER name="style" value="SUBSCRIPT"/>
    </ELEMENT_DISPLAY>
    <ELEMENT_DISPLAY element="i" type="style">
      <PARAMETER name="style" value="ITALIC"/>
    </ELEMENT_DISPLAY>
    <ELEMENT_DISPLAY element="b" type="style">
      <PARAMETER name="style" value="BOLD"/>
    </ELEMENT_DISPLAY>
