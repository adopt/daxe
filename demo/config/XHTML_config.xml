    </ELEMENT_DISPLAY>
    <ELEMENT_DISPLAY element="span" type="stylespan"/>
    <ELEMENT_DISPLAY element="strong" type="string"/>
    <ELEMENT_DISPLAY element="tt" type="string">
      <PARAMETER name="font" value="Monospaced"/>
    </ELEMENT_DISPLAY>
    <ELEMENT_DISPLAY element="abbr" type="string"/>
    <ELEMENT_DISPLAY element="acronym" type="string"/>
    <ELEMENT_DISPLAY element="address" type="string"/>
    <ELEMENT_DISPLAY element="br" type="empty"/>
    <ELEMENT_DISPLAY element="cite" type="string"/>
