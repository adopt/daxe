  <ELEMENTS_DISPLAY>
    <ELEMENT_DISPLAY element="html" type="division"/>
    <ELEMENT_DISPLAY element="head" type="division"/>
    <ELEMENT_DISPLAY element="title" type="string">
      <PARAMETER name="style" value="BOLD"/>
    </ELEMENT_DISPLAY>
    <ELEMENT_DISPLAY element="meta" type="empty">
      <PARAMETER name="titleAtt" value="name"/>
    </ELEMENT_DISPLAY>
    <ELEMENT_DISPLAY element="link" type="empty"/>
