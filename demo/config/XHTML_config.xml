  <MENUS>
    <MENU name="structure">
      <INSERT_MENU name="html" node_type="element"/>
      <INSERT_MENU name="head" node_type="element"/>
      <INSERT_MENU name="title" node_type="element"/>
      <INSERT_MENU name="meta" node_type="element"/>
      <INSERT_MENU name="link" node_type="element"/>
      <INSERT_MENU name="body" node_type="element"/>
    </MENU>
    <MENU name="divisions">
      <INSERT_MENU name="h1" node_type="element"/>
      <INSERT_MENU name="h2" node_type="element"/>
      <INSERT_MENU name="h3" node_type="element"/>
      <INSERT_MENU name="h4" node_type="element"/>
      <INSERT_MENU name="h5" node_type="element"/>
      <INSERT_MENU name="div" node_type="element"/>
      <INSERT_MENU name="hr" node_type="element"/>
    </MENU>
    <MENU name="texte">
      <INSERT_MENU name="p" node_type="element" shortcut="P"/>
      <INSERT_MENU name="a" node_type="element"/>
      <INSERT_MENU name="blockquote" node_type="element"/>
      <INSERT_MENU name="pre" node_type="element"/>
      <INSERT_MENU name="em" node_type="element"/>
      <INSERT_MENU name="span" node_type="element"/>
      <INSERT_MENU name="strong" node_type="element"/>
      <INSERT_MENU name="tt" node_type="element"/>
      <INSERT_MENU name="abbr" node_type="element"/>
      <INSERT_MENU name="acronym" node_type="element"/>
      <INSERT_MENU name="address" node_type="element"/>
      <INSERT_MENU name="br" node_type="element"/>
      <INSERT_MENU name="cite" node_type="element"/>
      <INSERT_MENU name="code" node_type="element"/>
      <INSERT_MENU name="style" node_type="element"/>
      <INSERT_MENU name="commentaire" node_type="comment"/>
    </MENU>
    <MENU name="listes_tables">
      <INSERT_MENU name="ul" node_type="element"/>
      <INSERT_MENU name="li" node_type="element"/>
      <INSERT_MENU name="ol" node_type="element"/>
      <INSERT_MENU name="dl" node_type="element"/>
      <INSERT_MENU name="dt" node_type="element"/>
      <INSERT_MENU name="dd" node_type="element"/>
