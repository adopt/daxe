      <PARAMETER name="style" value="BOLD"/>
      <PARAMETER name="font" value="Helvetica"/>
      <PARAMETER name="size" value="24"/>
    </ELEMENT_DISPLAY>
    <ELEMENT_DISPLAY element="h2" type="string">
      <PARAMETER name="style" value="BOLD"/>
      <PARAMETER name="size" value="16"/>
    </ELEMENT_DISPLAY>
    <ELEMENT_DISPLAY element="h3" type="string">
      <PARAMETER name="style" value="BOLD"/>
    </ELEMENT_DISPLAY>
    <ELEMENT_DISPLAY element="h4" type="string"/>
