      <INSERT_MENU name="option" node_type="element"/>
      <INSERT_MENU name="textarea" node_type="element"/>
    </MENU>
    <MENU name="styles">
      <FUNCTION_MENU function_name="jaxe.FonctionNormal" name="normal"/>
      <INSERT_MENU name="sup" node_type="element"/>
      <INSERT_MENU name="sub" node_type="element"/>
      <INSERT_MENU name="i" node_type="element"/>
      <INSERT_MENU name="b" node_type="element"/>
    </MENU>
